          {% if feature_item %}
            <div role="listitem" class="w-dyn-item">
              <a data-w-id="94c850ac-372e-178f-90d9-6542a6ab1793" href="news/{{ feature_item.slug }}.html" class="collection-link feature w-inline-block">
